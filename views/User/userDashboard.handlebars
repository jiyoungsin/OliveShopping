                    <img class="img-fluid" src="{{ this.ImagePath }}" alt="First">
                    <p style="color:white;">{{ this.title }}</p>
                    <p style="color:white;">{{ this.Desc }}</p>
                    <p style="color:white;">{{ this.Price }}</p>
                </div>
            {{/each}}
        </div>
    </div>
</section>

